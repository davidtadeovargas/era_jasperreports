	<queryString>
		<![CDATA[SELECT * FROM (SELECT partvta.VTA, partvta.CANT - partvta.CANTENTRE AS pend, partvta.PROD, partvta.DESCRIP, partvta.ALMA, partvta.UNID, partvta.CANT, partvta.CANTENTRE, emps.NOM, norefer, noser, partvta.TIPDOC, vtas.CODEMP, femi, vtas.SUCU, vtas.ESTAC, vtas.NOCAJ, vtas.ESTAD, vtas.IMPUE, tot FROM partvta LEFT OUTER JOIN vtas ON vtas.VTA = partvta.VTA LEFT OUTER JOIN emps ON CONCAT_WS('', emps.SER, emps.CODEMP ) = vtas.CODEMP WHERE partvta.idempresa = $P{idempresa} AND DATE(partvta.FALT) <> DATE(partvta.FENTRE) AND vtas.ESTAD = 'CO' AND vtas.TIPDOC  = 'FAC' AND CASE WHEN '$P!{EMP}'= '' THEN vtas.CODEMP = vtas.CODEMP ELSE vtas.CODEMP = '$P!{EMP}' END AND CASE WHEN '$P!{F_D}'= '' THEN vtas.FEMI = vtas.FEMI ELSE DATE(vtas.FEMI) >=  '$P!{F_D}' END AND CASE WHEN '$P!{F_A}' = '' THEN vtas.FEMI = vtas.FEMI ELSE DATE(vtas.FEMI) <= '$P!{F_A}' END)a WHERE pend <> 0 ORDER BY vta DESC]]>
