		<![CDATA[SELECT comprs.CODCOMP, partcomprs.CODCOM, partcomprs.CANT - partcomprs.DEVS AS pend, partcomprs.PROD, partcomprs.DESCRIP, partcomprs.ALMA, partcomprs.UNID, partcomprs.CANT, partcomprs.DEVS, provs.NOM, nodoc, noser, comprs.PROV, comprs.FALT, comprs.SUCU, comprs.ESTAC, comprs.NOCAJ, comprs.ESTADO, comprs.IMPUE, tot FROM partcomprs LEFT OUTER JOIN comprs ON partcomprs.CODCOM = comprs.CODCOMP LEFT OUTER JOIN provs ON CONCAT_WS('', provs.SER, provs.PROV ) = comprs.PROV WHERE partcomprs.idempresa = $P{idempresa} AND comprs.TIP = 'ORD' AND comprs.ESTADO = 'PE' AND CASE WHEN '$P!{EMP}'= '' THEN comprs.PROV = comprs.PROV ELSE comprs.PROV = '$P!{EMP}' END AND CASE WHEN '$P!{F_D}'= '' THEN comprs.FALT = comprs.FALT ELSE DATE(comprs.FALT) >=  '$P!{F_D}' END AND CASE WHEN '$P!{F_A}' = '' THEN comprs.FALT = comprs.FALT ELSE DATE(comprs.FALT) <= '$P!{F_A}' END  ORDER BY comprs.ID_ID DESC]]>
